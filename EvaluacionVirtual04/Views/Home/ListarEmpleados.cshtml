@model IEnumerable<EvaluacionVirtual04.Models.empleado>

@{
    ViewBag.Title = "ListarEmpleados";
}

<h2>ListarEmpleados</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.eCodigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eNombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eApellido)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eDNI)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eDireccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eCelular)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.eImagen)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.eCodigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eNombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eApellido)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eDNI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eDireccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eCelular)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.eImagen)
        </td>
        <td>
            @Html.ActionLink("Editar", "EditarEmpleado", new {  id=item.eCodigo }) |
            @Html.ActionLink("Details", "Details", new { id=item.eCodigo }) |
            @Html.ActionLink("Delete", "EliminarEmpleado", new { id=item.eCodigo })
        </td>
    </tr>
}

</table>
